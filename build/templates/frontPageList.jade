each item in items
	div.row 
		div.six.columns
			a(href="#", onClick='myEvent("' + item.repo.name + '")')
				= item.repo.name
		div.six.columns
			p Id: #{item.id}
	div.row
		div.six.columns
			p Type: #{item.type}
		div.six.columns
			p By: #{item.actor.login}
	div.row
		div.ten.columns
			p Public: #{item.public}
